<view class="content"><uni-nav-bar style="width:100%;" vue-id="1" left-icon="back" left-text="返回" title="动画绘本馆" fixed="true" data-event-opts="{{[['^clickLeft',[['pageBack']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-nav-bar><view class="status_bar"></view><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{bannerList}}" wx:for-item="item" wx:for-index="index" wx:key="bannerCode"><swiper-item><view class="swiper-item"><image src="{{item.bannerImageUrl}}"></image></view></swiper-item></block></swiper><view class="wiki"><block wx:for="{{wikiList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="wiki_item"><image src="{{item.imageUrl}}"></image><text>{{item.sortTitle}}</text></view></block></view><view class="block"><view class="header">今日推荐</view><image class="recommend_image" src="{{subjectDetails.topicImg}}"></image><text class="topicTitle">{{subjectDetails.topicTitle}}</text></view><view class="block"><view class="header">免费专区</view><view class="book_list"><block wx:for="{{bookList}}" wx:for-item="item" wx:for-index="index" wx:key="bookCode"><view class="book_item"><image src="{{item.coverUrl}}"></image><text>{{item.bookName}}</text></view></block></view></view></view>